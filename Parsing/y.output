    6          | function
    7          | coordinator

    8 tuple: Tuple Type_Name ':' element_defs

    9 element_defs: element_def
   10             | element_def element_defs

   11 element_def: names ':' static_type

   12 names: Variable_Name
   13      | Variable_Name ',' names

   14 static_type: scalar_type
   15            | static_array

   16 scalar_type: T_Integer
   17            | T_Real Single
   18            | T_Real Double
   19            | T_Character
   20            | T_Boolean
   21            | T_Epoch
   22            | T_Range
   23            | Type_Name

   24 static_array: T_Array static_dims Of scalar_type

   25 static_dims: '[' Integer ']' static_dims
   26            | '[' Integer ']'

   27 task: task_header task_body

   28 task_header: Task String ':'

   29 task_body: define environment initialize compute partition

   30 define: S_Define ':' definitions

   31 definitions: definition
   32            | definition definitions

   33 definition: names ':' type

   34 type: static_type
   35     | dynamic_type

   36 dynamic_type: list
   37             | dynamic_array

   38 list: T_List Of static_type

   39 dynamic_array: Dimensionality T_Array Of scalar_type format

   40 format: /* empty */
   41       | Format Type_Name

   42 environment: S_Environment ':' linkages

   43 linkages: linkage
   44         | linkage linkages

   45 linkage: names ':' mode

   46 mode: Link
   47     | Create
   48     | Link_or_Create

   49 initialize: /* empty */
   50           | init_header ':' code

   51 init_header: S_Initialize arguments

   52 arguments: /* empty */
   53          | '(' names ')'

   54 compute: S_Compute ':' meta_stages

   55 meta_stages: stage_sequence
   56            | meta_stage
   57            | meta_stage meta_stages

   58 meta_stage: stage_sequence repeat_control

   59 stage_sequence: compute_stage
   60               | compute_stage stage_sequence

   61 compute_stage: stage_header '{' stage_body '}'

   62 stage_header: String '(' Space Space_ID ')' activation_command

   63 activation_command: /* empty */
   64                   | Activate If expr In expr

   65 repeat_control: Repeat ':' From String repeat_loop

   66 stage_body: meta_stages
   67           | code

   68 partition: S_Partition arguments ':' partition_specs

   69 partition_specs: partition_spec
   70                | partition_spec partition_specs

   71 partition_spec: Space Space_ID '<' Dimensionality '>' attributes divides '{' data_dist '}'
   72               | Space Space_ID '<' Unpartitioned '>' '{' names '}'

   73 attributes: /* empty */
   74           | '<' attribute '>' attributes

   75 attribute: Dynamic

   76 divides: /* empty */
   77        | Divides Space Space_ID parent_config

   78 parent_config: Partitions
   79              | Sub_Partitions

   80 data_dist: main_dist sub_dist

   81 main_dist: data_spec
   82          | data_spec main_dist

   83 data_spec: var_list ':' instr_list relativity

   84 var_list: var
   85         | var ',' var_list

   86 var: Variable_Name
   87    | Variable_Name '<' dimensions '>'

   88 dimensions: V_Dimension
   89           | V_Dimension ',' dimensions

   90 instr_list: instr
   91           | instr ',' instr_list

   92 instr: Replicated
   93      | Variable_Name '(' partition_args ')' padding

   94 padding: /* empty */
   95        | Padding '(' partition_args ')'

   96 partition_args: /* empty */
   97               | partition_arg
   98               | partition_arg ',' partition_args

   99 partition_arg: Variable_Name
  100              | Integer

  101 relativity: /* empty */
  102           | ';' Relative_To Space Space_ID

  103 sub_dist: /* empty */
  104         | P_Sub_Partition '<' Dimensionality '>' '<' nature '>' '{' data_sub_dist '}'

  105 nature: Ordered
  106       | Unordered

  107 data_sub_dist: data_sub_spec
  108              | data_sub_spec data_sub_dist

  109 data_sub_spec: var_list ':' ordered_instr_list

  110 ordered_instr_list: ordered_instr
  111                   | ordered_instr ',' ordered_instr_list

  112 ordered_instr: instr order

  113 order: /* empty */
  114      | Ascends
  115      | Descends

  116 repeat_loop: For expr In expr step_expr
  117            | While expr
  118            | Foreach Space Space_ID C_Sub_Partition

  119 step_expr: /* empty */
  120          | Step expr

  121 $@1: /* empty */

  122 code: $@1 stmt_block

  123 stmt_block: stmt
  124           | stmt new_lines
  125           | stmt new_lines stmt_block

  126 new_lines: New_Line
  127          | New_Line new_lines

  128 stmt: parallel_loop
  129     | sequencial_loop
  130     | if_else_block
  131     | expr

  132 sequencial_loop: Do In Sequence '{' stmt_block '}' For expr In expr step_expr

  133 parallel_loop: Do '{' stmt_block '}' For index_ranges
  134              | Do '{' stmt_block '}' While expr

  135 index_ranges: index_range
  136             | index_range ';' index_ranges

  137 index_range: names In Variable_Name restrictions

  138 restrictions: /* empty */
  139             | O_AND expr

  140 if_else_block: If '(' expr ')' '{' stmt_block '}' else_block

  141 else_block: /* empty */
  142           | else
  143           | else_if else_block

  144 else: Else '{' stmt_block '}'

  145 else_if: Else If '(' expr ')' '{' stmt_block '}'

  146 expr: expr '+' expr
