Tuple Man:
	age: Integer
	income: Real single-precision

Function checkStatus:
	Arguments:
		man: Man
		woman: Man
	Results:
		match: Boolean
		happiness: Real	single-precision
	Compute:
		cost = man.income / woman.age
		if (cost > 1) {
			savings = cost > 1
		}
		match = savings && (woman.income > 100)
		happiness = man.income - cost
		
	 
